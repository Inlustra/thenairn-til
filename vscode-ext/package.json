{
  "name": "tilas",
  "displayName": "tilas",
  "description": "Create TILs for Tilas without leaving your editor",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.39.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "gulp",
    "compile": "tsc -p ./",
    "watch:tsc": "tsc -watch -p ./",
    "watch:gulp": "gulp watch",
    "watch": "npm-run-all -p -l watch:tsc watch:gulp",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/gulp": "^4.0.6",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.39.0",
    "glob": "^7.1.4",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "gulp-merge-json": "^1.3.1",
    "gulp-typescript": "^5.0.1",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.4.1",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode-test": "^1.2.0"
  },
  "dependencies": {
    "apollo-boost": "^0.4.4",
    "apollo-client": "^2.6.4",
    "apollo-link-context": "^1.0.19",
